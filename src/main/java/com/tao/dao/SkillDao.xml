<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tao.dao.SkillDao">
    <select id="selectSkill" resultType="com.tao.domain.Skill">
        select * from skill where skill_name = #{skill_name}
    </select>

</mapper>